<div>
  <main>
    <div>
      {{#with page_header}}
        <div class='text-center'><h1>{{title}}</h1></div>
        <p
          class='text-uppercase text-center'
        >{{description}}</p>

        <hr />
      {{/with}}
    </div>
    <div class='card'>
      <div class='card-body'>
        {{#with section}}
          <div class='card'>
            <div class='card-body'>
              {{#with about_us}}
                <div
                  class='display-6 text-end'
                >{{title_list.main}}</div>
                <br />
                <div class='text-end'><h2
                  >{{title_list.sub}}</h2>
                </div>
                <br />
                <button
                  class='btn btn-outline-info'
                >{{button.text}}</button>

              {{/with}}
            </div>
          </div>
          <br />
          <br />

          <div class='card'>
            <div class='card-body'>
              {{#with advantage}}
                <div class='display-5'>{{title}}</div>
                <br />
                <div class='card'>
                  <div class='card-body'>
                    {{#with efficiency}}
                      <div class='display-4'>{{title}}</div>
                      <br />
                      <h6>{{sub_title}}</h6>
                      <br />
                      <p>{{description}}</p>
                      <br />
                      <button
                        class='btn btn-outline-warning'
                      >{{button}}</button>
                    {{/with}}
                  </div>
                </div>

                <br />
                <br />

                <div class='card'>
                  <div class='card-body'>
                    {{#with innovation}}
                      <div
                        class='display-4 text-center'
                      >{{title}}</div>
                      <br />
                      <div class='text-center'><h6
                        >{{sub_title}}</h6></div>
                      <br />
                      <p
                        class='text-center'
                      >{{description}}</p>
                      <br />
                      <button
                        class='btn btn-outline-warning'
                      >{{button}}</button>

                    {{/with}}
                  </div>
                </div>

                <br />
                <br />

                <div class='card'>
                  <div class='card-body'>
                    {{#with collaboration}}
                      <div
                        class='display-4 text-end'
                      >{{title}}</div>
                      <br />
                      <div class='text-end'><h6
                        >{{sub_title}}</h6></div>
                      <br />
                      <p
                        class='text-end'
                      >{{description}}</p>
                      <br />
                      <button
                        class='btn btn-outline-warning'
                      >{{button}}</button>

                    {{/with}}
                  </div>
                </div>
              {{/with}}
            </div>
          </div>

          <br />
          <br />

          <div class='card'>
            <div class='card-body'>
              {{#with agreement}}
                <div class='display-5'>{{title}}</div>
                <br />
                <div><h5>{{info.sub_title}}</h5></div>
                <hr />
                <br />
                {{#with info.detail}}
                  <p>{{text}}</p>
                  <br />
                  <button
                    class='btn btn-success'
                  >{{button}}</button>
                {{/with}}

              {{/with}}
            </div>
          </div>

        {{/with}}
      </div>
    </div>
  </main>
</div>